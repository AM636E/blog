<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zaz\BlogBundle\Entity\Role" table="role">
    <id name="roleId" type="integer" column="role_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="256" nullable="false"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <many-to-many field="user" target-entity="User" mapped-by="role"/>
  </entity>
</doctrine-mapping>
