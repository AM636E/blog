<div class="posts panel-container">
  {% for post in posts %}
    <div class="panel panel-default">
      <div class="post post-title panel-heading">
        <a href="/post/{{ post.getId }}">
          {{ post.title }}
        </a>
      </div>
      <div class="post-wrapper panel-body">
        <div class="post post-content">
          {{ post.content }}
        </div>
      </div>
    </div>
  {% endfor %}
</div>