<div class="posts">
  {% for post in posts %}
    <div class="post-wrapper">
      <div class="post post-title">
        <a href="/post/{{ post.getId }}">
          {{ post.title }}
        </a>
      </div>
      <div class="post post-content">
        {{ post.content }}
      </div>
    </div>
  {% endfor %}
</div>